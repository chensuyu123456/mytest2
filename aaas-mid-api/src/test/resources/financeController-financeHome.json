{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "financeHome",
  "description": "理财首页",
  "type": "object",
  "properties": {
    "head": {
      "type": "object",
      "properties": {
        "status": {
          "type": [
            "string",
            "null"
          ]
        },
        "msg": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "title": "返回头信息"
    },
    "result": {
      "type": "object",
      "properties": {
        "uuid": {
          "type": [
            "string",
            "null"
          ]
        },
        "name": {
          "type": "string"
        },
        "banner_list": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "productTypeList": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "investmentHorizonCode": {
                "type": "string"
              },
              "investmentHorizon": {
                "type": "string"
              },
              "id":{
                "type":"string"
              }
            },
            "required": [
              "investmentHorizonCode",
              "investmentHorizon",
              "id"
            ]
          }
        },
        "C3": {
          "type": "object",
          "properties": {
            "income6month": {
              "type": "array",
              "properties": {
                "_total": {
                  "type": "integer"
                },
                "_items": {
                  "type": "array"
                },
                "name": {
                  "type": [
                    "string",
                    "null"
                  ]
                },
                "_links": {
                  "type": ["object","null"]
                },
                "maxMinMap": {
                  "type": ["object","null"]
                },
                "maxMinBenchmarkMap": {
                  "type": ["object","null"]
                },
                "expectedIncomeSizeMap": {
                  "type": ["object","null"]
                },
                "highPercentMaxIncomeSizeMap": {
                  "type": ["object","null"]
                },
                "highPercentMinIncomeSizeMap": {
                  "type": ["object","null"]
                },
                "lowPercentMaxIncomeSizeMap": {
                  "type": ["object","null"]
                },
                "lowPercentMinIncomeSizeMap": {
                  "type": ["object","null"]
                },
                "_schemaVersion": {
                  "type": [
                    "string",
                    "null"
                  ]
                },
                "_serviceId": {
                  "type": [
                    "string",
                    "null"
                  ]
                }
              },
              "required": [
                "_total",
                "_items",
                "name",
                "_links",
                "maxMinMap",
                "maxMinBenchmarkMap",
                "expectedIncomeSizeMap",
                "highPercentMaxIncomeSizeMap",
                "highPercentMinIncomeSizeMap",
                "lowPercentMaxIncomeSizeMap",
                "lowPercentMinIncomeSizeMap",
                "_schemaVersion",
                "_serviceId"
              ]
            },
            "groupId": {
              "type": [
                "string",
                "null"
              ]
            },
            "subGroupId": {
              "type": [
                "string",
                "null"
              ]
            },
            "name": {
              "type": [
                "string",
                "null"
              ]
            },
            "historicalYearPerformance": {
              "type": "string"
            },
            "product_list": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": [
                      "string",
                      "null"
                    ]
                  },
                  "value": {
                    "type": "integer"
                  }
                }
              }
            },
            "historicalvolatility": {
              "type": "string"
            }
          },
          "required": [
            "income6month",
            "groupId",
            "subGroupId",
            "name",
            "historicalYearPerformance",
            "product_list",
            "historicalvolatility"
          ]
        }
      },
      "required": [
        "uuid",
        [ "C1","C2","C3","C4","C5"],
        "banner_list",
        "name",
        "productTypeList"
      ]
    }
  },
  "required": [
    "head",
    "result"
  ]
}