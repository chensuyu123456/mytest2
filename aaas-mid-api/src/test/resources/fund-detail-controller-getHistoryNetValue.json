{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Net value of history",
  "description": "历史净值",
  "type": "object",
  "properties": {
    "head": {
      "type": "object",
      "properties": {
        "status": {
          "type": [
            "string",
            "null"
          ]
        },
        "msg": {
          "type": [
            "string",
            "null"
          ]
        }
      },
      "title": "返回头信息"
    },
    "result": {
      "type": "object",
      "properties": {
        "period": {
          "type": "string"
        },
        "code": {
          "type": "string"
        },
        "historylist": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "date": {
                "type": "string"
              },
              "navaccum": {
                "type": "number"
              },
              "dayup": {
                "type": "string"
              },
              "navunit": {
                "type": "number"
              }
            },
            "required": [
              "date",
              "navaccum",
              "dayup",
              "navunit"
            ]
          }
        }
      },
      "required": [
        "historylist"
      ]
    }
  },
  "required": [
    "head",
    "result"
  ]
}
